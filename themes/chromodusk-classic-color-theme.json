{
    "name": "Chromodusk Classic",
	"include": "./chromodusk-color-theme.json",
    "color": {
        // These don't work appearently, but i'll keep them here for reference.
        "editor.background": "#000205",
        // "editor.background": "#000000",
        "editorBracketHighlight.foreground1": "#ffd700",
		"editorBracketHighlight.foreground2": "#da70d6",
		"editorBracketHighlight.foreground3": "#179fff",
        "editorBracketHighlight.foreground4": "#ffd700",
		"editorBracketHighlight.foreground5": "#da70d6",
		"editorBracketHighlight.foreground6": "#179fff",
    },
    "semanticTokenColors": {
      "selfParameter": "#E8364F",
      "selfKeyword": "#E8364F",
      "clsParameter": "#E8364F",
      "selfTypeKeyword": "#E8364F",
    //   "*.typeHint": "#C71585",
    //   "*.callable": "#00FF00",
    //   "tomlTableKey": "#C71585",
    //   "tomlArrayKey": "#C71585",
    //   "derive": "#66cdaa"
    },
    "tokenColors": [
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "scope": ["entity.name.function", "meta.function-call.generic"],
            "settings": {
                "foreground": "#A6E22E",
            }
        },
        {
            "scope": [
                "entity.name.function.preprocessor",
                "entity.name.function.macro"
            ],
            "settings": {
                "foreground": "#A6E22E",
                // "fontStyle": "underline"
            }
        },
        {
            "scope": ["storage", "storage.type", "keyword"],
            "settings": {
                "foreground": "#E8364F"
            }
        },
        {
            "scope": ["string", "string punctuation.section.embedded source"],
            "settings": {
				"foreground": "#D3C970"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
				"foreground": "#9A79D7"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
				"foreground": "#9A79D7"
            }
        },
        {
            "scope": "support.type",
            "settings": {
				"foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.function",
            "settings": {
				"foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.variable",
            "settings": {
				"foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
				"foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
			"scope": "variable.other.rust",
			"settings": {
				"foreground": "#C6C6C6"
			}
		},
        {
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#C6C6C6"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#747475"
			}
		}
    ]
}