{
    "name": "Chromodusk Classic",
	"include": "./chromodusk-color-theme.json",
    "color": {
        // These don't work appearently, but i'll keep them here for reference.
        "editor.background": "#000205",
        // "editor.background": "#000000",
        "editorBracketHighlight.foreground1": "#ffd700",
		"editorBracketHighlight.foreground2": "#da70d6",
		"editorBracketHighlight.foreground3": "#179fff",
        "editorBracketHighlight.foreground4": "#ffd700",
		"editorBracketHighlight.foreground5": "#da70d6",
		"editorBracketHighlight.foreground6": "#179fff",
    },
    "semanticTokenColors": {
        "selfParameter": "#B0AF95",
        "selfKeyword": "#E8364F",
        "clsParameter": "#E8364F",
        "selfTypeKeyword": "#E8364F",
        "*.typeHint": "#66D9EF",
        "deriveHelper.attribute": "#66CDAA",
        "attributeBracket.attribute": "#DEB887",
        "comment.documentation": "#00FF7F", // strange this has to go through semantic
    //   "*.callable": "#00FF00",
    //   "tomlTableKey": "#C71585",
    //   "tomlArrayKey": "#C71585",
    //   "derive": "#66cdaa"
    },
    "tokenColors": [
		{
			"scope": "text, source",
			"settings": {
				"foreground": "#C6C6C6"
			}
		},
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#747475"
			}
		},
        {
            "scope": "comment.line.documentation",
            "settings": {
                "foreground": "#00FF7F"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "scope": [
                "punctuation.separator",
                "punctuation.definition.template-expression",
                "punctuation.accessor",
                "punctuation.section.function",
                "punctuation.definition.string.template"
            ],
            "settings": {
                "foreground": "#C6C6C6",
                "fontStyle": ""
            }
        },
        {
            "scope": ["entity.name.function", "meta.function-call.generic"],
            "settings": {
                "foreground": "#A6E22E",
            }
        },
        {
            "scope": [
                "entity.name.function.preprocessor",
                "entity.name.function.macro"
            ],
            "settings": {
                "foreground": "#A6E22E",
                // "fontStyle": "underline"
            }
        },
        {
            "scope": "storage.type.string.python",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "scope": ["storage", "storage.type", "keyword"],
            "settings": {
                "foreground": "#E8364F"
            }
        },
        {
            "scope": "storage.type.function.python",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "scope": ["string", "string punctuation.section.embedded source"],
            "settings": {
                "foreground": "#D3C970"
            }
        },
        {
            "scope": [
                "constant.language", 
                "constant.numeric",
                "constant.character"
            ],
            "settings": {
                "foreground": "#9A79D7"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#C6C6C6"
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#9A79D7",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other.rust",
			"settings": {
                "foreground": "#C6C6C6"
			}
		},
        {
            // for np.BUFSIZE
            // doesn't work; conflict with python properties
            "scope": "variable.other.property",
			"settings": {
				"foreground": "#C6C6C6",
                // "fontStyle": "underline"
			}
		},
        {
            "scope": "support.function.magic",
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": ""
            }
        },
        {
            "scope": "entity.name.type.enum",
            "settings": {
                // "foreground": "#895fff"
                // "foreground": "#c76cf4"
                "foreground": "#7897FF"
            }
        },
        {
            "scope": [
                "punctuation.brackets.round.rust meta.function.definition.rust"
            ],
            "settings": {
                "foreground": "#A6E22E"
            }
        },
        {
            "scope": "meta.attribute.rust punctuation.comma.rust",
            "settings": {
                "foreground": "#66CDAA",
            }
        },
        {
            "scope": [
                "constant.character.format", 
                "storage.type.format.python"
            ],
            "settings": {
                "foreground": "#FF7B72"
            }
        }
    ]
}